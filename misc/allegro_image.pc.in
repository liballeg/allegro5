prefix=@prefix@
exec_prefix=${prefix}
libdir=@libdir@
includedir=@includedir@
version=@A5O_VERSION@
suffix=@lib_type@@lib_linkage@
versuffix=@versuffix@

Name: allegro_image
Description: Allegro game programming library, image I/O addon
Version: ${version}
